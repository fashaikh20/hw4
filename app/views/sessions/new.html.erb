<h3>Login</h3>

<%= form_with url: "/sessions" do |form| %>
  
  <div class="mb-3">
    <%= form.label "email" %>
    <%= form.text_field "email" %>
  </div>

  <div class="mb-3">
    <%= form.label "password" %>
    <%= form.password_field "password" %>
  </div>

  <%= form.submit "Login", class: "btn btn-primary" %>
<% end %>